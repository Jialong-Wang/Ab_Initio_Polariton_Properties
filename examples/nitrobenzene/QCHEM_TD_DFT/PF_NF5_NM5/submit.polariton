#!/bin/bash
#SBATCH -p debug
#SBATCH -J PF
#SBATCH -o output.slurm
#SBATCH -t 1:00:00
#SBATCH -N 1
#SBATCH --ntasks-per-node=1
#SBATCH --mem 1GB

export OMP_NUM_THREADS=1
export MKL_NUM_THREADS=1

for k in {0..1000..10}; do
    wc=$( bc -l <<< $k*0.01 )
    echo "A0 = $1 wc = $wc"
    python3 Pauli-Fierz_DdotE.py $1 ${wc}
done

